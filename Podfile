end

target 'watch' do
  platform :watchos, '3.0'
  pod 'SwiftyJSON'
end

target 'watch Extension' do
  platform :watchos, '3.0'
  pod 'Alamofire', '~> 4.7'
  pod 'SwiftyJSON'
  pod 'CryptoSwift', :git => 'https://github.com/krzyzanowskim/CryptoSwift.git', :branch => 'swift4'
end

